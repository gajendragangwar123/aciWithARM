{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "noCpu": {
            "value": "1"
        },
        "memoryInGB": {
            "value": "4"
        },
        "registryServerName": {
            "defaultValue": "gajndraserver",
            "type": "string"
        },
        "registryUserName": {
            "defaultValue": "gajndrauser",
            "type": "string"
        },
        "registryPassword": {
            "defaultValue": "gajndrapassword",
            "type": "string"
        },
        "image": {
            "defaultValue": "gajndraimage",
            "type": "string"
        }
    }
}
